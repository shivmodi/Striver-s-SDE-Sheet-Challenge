#include <bits/stdc++.h> 
vector<vector<long long int>> printPascal(int n) 
{
  // Write your code here.
    vector<vector<long long int>> v(n);
    v[0].push_back(1);
    for(int i=1;i<n;i++){
        v[i].push_back(1);
         for(int j=1;j<v[i-1].size();j++){
             v[i].push_back((v[i-1][j]+v[i-1][j-1]));
         }
        v[i].push_back(1);
    }
    return v;
}
